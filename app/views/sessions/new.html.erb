<div class="col-lg-3 col-lg-offset-3">
  <h1>Log in</h1>
  <form action='/sessions', method='post' class'form-horizontal' id='login_form'>
    <div class="form-group">
      <label for='email'>Email:</label>
      <input type='email' class='form-control' id='email' name='sessions[email]'>
    </div>
    <div class="form-group">
      <label for='password'>Password:</label>
      <input type='password' class='form-control' id='password' name='sessions[password]'>
    </div>
    <button type='submit' class='btn btn-primary' >Log in</button>
  </form>
  <%= link_to 'Back', users_path %>
</div>
